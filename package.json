{
	"name": "@tuplo/yaintl",
	"description": "Yet another i18n library with ICU message syntax",
	"version": "0.0.0-development",
	"repository": "git@github.com:tuplo/yaintl.git",
	"author": "Rui Costa",
	"license": "MIT",
	"keywords": [
		"i18n",
		"intl",
		"ICU",
		"messageformat"
	],
	"types": "dist/index.d.ts",
	"module": "./dist/index.mjs",
	"main": "./dist/index.cjs",
	"exports": {
		".": [
			{
				"import": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.mjs"
				},
				"require": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.cjs"
				},
				"default": "./dist/index.mjs"
			},
			"./dist/index.mjs"
		]
	},
	"files": [
		"dist/index.mjs",
		"dist/index.cjs",
		"dist/index.d.ts"
	],
	"scripts": {
		"build": "tsx sh/build.ts",
		"coverage": "tsx sh/coverage.ts",
		"format": "prettier --write src sh",
		"lint:ts": "tsc --noEmit",
		"lint": "eslint src --ext ts,tsx",
		"run:examples:react": "tsx sh/run-examples-react.ts",
		"test:ci": "TZ=utc vitest run",
		"test": "TZ=utc vitest --watch"
	},
	"dependencies": {
		"@ffz/icu-msgparser": "2.0.0",
		"dlv": "1.1.3"
	},
	"devDependencies": {
		"@tuplo/shell": "1.2.2",
		"@types/dlv": "1.1.4",
		"@types/node": "20.10.4",
		"@types/react": "18.2.43",
		"@types/react-dom": "18.2.17",
		"@typescript-eslint/eslint-plugin": "6.13.2",
		"@typescript-eslint/parser": "6.13.2",
		"@vitest/coverage-v8": "1.0.4",
		"esbuild": "0.19.9",
		"eslint": "8.55.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-import": "2.29.0",
		"npm-check-updates": "16.14.11",
		"nyc": "15.1.0",
		"prettier": "3.1.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"tsx": "4.6.2",
		"typescript": "5.3.3",
		"vitest": "1.0.4"
	}
}
