{
	"name": "@tuplo/yaintl",
	"description": "Yet another i18n library with ICU message syntax",
	"version": "0.0.0-development",
	"repository": "git@github.com:tuplo/yaintl.git",
	"author": "Rui Costa",
	"license": "MIT",
	"keywords": [
		"i18n",
		"intl",
		"ICU",
		"messageformat"
	],
	"types": "dist/index.d.ts",
	"module": "./dist/index.mjs",
	"main": "./dist/index.cjs",
	"exports": {
		".": [
			{
				"import": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.mjs"
				},
				"require": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.cjs"
				},
				"default": "./dist/index.mjs"
			},
			"./dist/index.mjs"
		]
	},
	"files": [
		"dist/index.mjs",
		"dist/index.cjs",
		"dist/index.d.ts"
	],
	"scripts": {
		"build": "tsx sh/build.ts",
		"coverage": "tsx sh/coverage.ts",
		"format": "prettier --write src sh",
		"lint:ts": "tsc --noEmit",
		"lint": "eslint src --ext ts,tsx",
		"test:ci": "TZ=utc vitest run",
		"test": "TZ=utc vitest --watch"
	},
	"dependencies": {
		"@ffz/icu-msgparser": "2.0.0",
		"dlv": "1.1.3"
	},
	"devDependencies": {
		"@tuplo/shell": "1.2.0",
		"@types/dlv": "1.1.2",
		"@types/node": "20.4.9",
		"@typescript-eslint/eslint-plugin": "6.3.0",
		"@typescript-eslint/parser": "6.3.0",
		"@vitest/coverage-v8": "0.34.1",
		"esbuild": "0.19.0",
		"eslint": "8.46.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-import": "2.28.0",
		"nyc": "15.1.0",
		"prettier": "3.0.1",
		"tsx": "3.12.7",
		"typescript": "5.1.6",
		"vitest": "0.34.1"
	}
}
